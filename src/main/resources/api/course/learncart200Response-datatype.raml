#%RAML 1.0 DataType
type: array
items:
  type: object
  properties:
    name:
      type: string
      required: true
      example: "Harry"
    author: 
      type: string
      required: true
      example: "JK"
    duration:
      type: string
      example: "2hrs"
      required: true
    price: 
      type: integer
      required: true
      example: 199