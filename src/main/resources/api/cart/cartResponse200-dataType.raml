#%RAML 1.0 DataType
type: object
properties:
  total_price:
    type: integer
    required: true
    example: 1234
  cart-items:
    type: array
    items:
      type: object
      properties:
        courseid: 
          type: string
          required: true
          example: "12"
        coursename:
          type: string
          required: true
          example: "Java"
        courseprice:
          type: number
          required: true
          example: 2000